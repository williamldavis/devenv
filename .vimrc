set nocompatible

syntax enable on

filetype plugin on

colorscheme luther

set autochdir
set background=dark
set cindent
set cursorcolumn
set cursorline
set expandtab
set guifont=Consolas\ 11
set ignorecase
set incsearch
set nowrap
set number
set ruler
set shiftwidth=3
set showcmd
set smartcase
set softtabstop=3
set tabstop=3
"set termencoding=utf-8
set visualbell
